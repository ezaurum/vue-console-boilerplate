    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "istanbul": "^0.4.5",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-markdown": "^1.2.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
