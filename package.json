{
  "name": "congkong-admin",
  "version": "3.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "build:participant": "vue-tsc --noEmit && VITE_BUILD_TARGET='participant' vite build",
    "build:admin": "vue-tsc --noEmit && VITE_BUILD_TARGET='admin' vite build",
    "test:unit": "mocha --ui bdd",
    "dev": "vite --host",
    "dev:participant": "VITE_BUILD_TARGET='participant' vite --host",
    "dev:admin": "VITE_BUILD_TARGET='admin' vite --host",
    "lint:script": "eslint --ext .ts,vue,js,tsx,jsx --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@formvuelate/plugin-vee-validate": "^3.8.1",
    "@vueuse/core": "^7.6.2",
    "@vueuse/router": "^7.6.2",
    "axios": "^0.26.0",
    "axios-middleware": "^0.3.1",
    "cox-postposition": "^1.4.1",
    "formvuelate": "^3.8.1",
    "gsap": "^3.9.1",
    "howler": "^2.2.3",
    "mitt": "^3.0.0",
    "sweetalert2": "^11.4.4",
    "unocss": "^0.26.1",
    "vee-validate": "^4.5.8",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
