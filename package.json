    "vue-tsc": "^0.31.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue,js,tsx,jsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix"
  },
  "resolutions": {
    "@types/react": "file:stub/types__react",
    "@types/react-syntax-highlighter": "file:stub/types__react"
  }
}
